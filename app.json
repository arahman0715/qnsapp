{
  "expo": {
    "name": "QNS App",
    "slug": "qnsapp",
    "description": "Quran and Hadith book App",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#7f70e7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "app/assets/*"
    ],
    "android": {
      "package": "org.qnsbd.qnsapp"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.qnsbd.qnsapp"
    },
    "githubUrl": "https://github.com/mahmed0715/qnsapp"
  }
}
